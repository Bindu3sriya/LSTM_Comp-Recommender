   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.feature_extraction.text import TfidfVectorizer\n",
    "from sklearn.preprocessing import MultiLabelBinarizer\n",
    "from keras.models import Model\n",
    "from keras.layers import Input, LSTM, Dense, Dropout, Embedding, Concatenate, BatchNormalization\n",
    "from keras.preprocessing.text import Tokenizer\n",
    "from keras.preprocessing.sequence import pad_sequences\n",
    "from keras.optimizers import Adam\n",
    "import tensorflow as tf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 107,
   "id": "54265e9d-f4d4-4453-9d19-82285dd4390b",
   "metadata": {},
   "outputs": [],
   "source": [
    "df  = pd.read_csv('HADOOP3000.csv')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
   "id": "db86cb27-d874-42b1-99ad-575528306a48",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Assuming 'title' and 'description' are the columns in your DataFrame 'df'\n",
    "df['title'] = df['title'].fillna('missing')  # Replace NaN with 'missing'\n",
    "df['description'] = df['description'].fillna('missing')  # Replace NaN with 'missing'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 109,
   "id": "c3430e09-4650-4b29-869a-667d93e03ab7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Ensure both columns are of string type\n",
    "df['title'] = df['title'].astype(str)\n",
    "df['description'] = df['description'].astype(str)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 110,
   "id": "3893bd24-18c7-47cc-80c2-95bf40b1c37f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Now concatenate 'title' and 'description'\n",
    "df['text'] = df['title'] + ' ' + df['description']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 114,
   "id": "3f3f1bbe-4482-499b-ac17-a82cce46dc5e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Ensure y is aligned with the reduced df\n",
    "y = y[:len(df)]\n" #y is target variable
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 115,
   "id": "f80f3491-4905-45e5-9572-e8b3efb0e7b7",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Assuming df['text'] and y are now aligned\n",
    "X_train, X_test, y_train, y_test = train_test_split(df['text'], y, test_size=0.2, random_state=42)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 116,
   "id": "b2ed2899-88cf-409d-ad6e-7f8e5d12bb57",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Tokenize texts for LSTM\n",
    "tokenizer = Tokenizer()\n",
    "tokenizer.fit_on_texts(X_train)  # Now X_train should only contain strings\n",
    "X_train_seq = tokenizer.texts_to_sequences(X_train)\n",
    "X_test_seq = tokenizer.texts_to_sequences(X_test)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 117,
   "id": "bde870eb-94a4-4c5c-b648-16fc1f7a53cc",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Pad sequences\n",
    "max_length = max(len(s) for s in X_train_seq)\n",
    "X_train_pad = pad_sequences(X_train_seq, maxlen=max_length, padding='post')\n",
    "X_test_pad = pad_sequences(X_test_seq, maxlen=max_length, padding='post')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 118,
   "id": "2b4b3316-e3d6-49bb-8788-68b1e21b516e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Feature extraction: TF-IDF for textual similarity features\n",
    "tfidf = TfidfVectorizer(max_features=1000)\n",
    "X_train_tfidf = tfidf.fit_transform(X_train).toarray()\n",
    "X_test_tfidf = tfidf.transform(X_test).toarray()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 119,
   "id": "68986217-7d00-4cc1-bb2e-e8a08fd92084",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define model inputs\n",
    "input_text = Input(shape=(max_length,))\n",
    "input_tfidf = Input(shape=(X_train_tfidf.shape[1],))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 120,
   "id": "74cb24c0-511c-4bed-8538-6d34bb287c21",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Embedding layer for text input\n",
    "emb = Embedding(input_dim=len(tokenizer.word_index) + 1, output_dim=50, input_length=max_length)(input_text)\n",
    "lstm_layer = LSTM(64)(emb)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 121,
   "id": "d0d4a266-98e0-4e14-b15a-4fae51c54c17",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Combine LSTM output with TF-IDF input\n",
    "combined = Concatenate()([lstm_layer, input_tfidf])\n",
    "combined = BatchNormalization()(combined)\n",
    "combined = Dense(64, activation='relu')(combined)\n",
    "combined = Dropout(0.5)(combined)\n",
    "output = Dense(y.shape[1], activation='sigmoid')(combined)\n",
    "\n",
    "model = Model(inputs=[input_text, input_tfidf], outputs=output)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 125,
   "id": "080aabf1-ef49-4231-a698-edcc0f59c0f4",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Compile the model\n",
    "model.compile(optimizer=Adam(learning_rate=0.01, beta_1=0.9, epsilon=1e-6), loss='binary_crossentropy', metrics=['accuracy'])"
   ]
  },
